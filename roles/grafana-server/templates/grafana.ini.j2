[server]
protocol = http
http_addr = 127.0.0.1
http_port = 3000
domain = grafana.fsicardi.ar
enforce_domain = true
root_url = https://grafana.fsicardi.ar
enable_gzip = true

[smtp]
enabled = true
host = smtp.gmail.com:465
user = {{ gmail_address }}
password = """{{ gmail_password }}"""
skip_verify = true
from_address = {{ grafana_address }}
from_name = Alerting
